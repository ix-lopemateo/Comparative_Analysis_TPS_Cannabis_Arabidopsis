   "source": [
    "with open(\"tree.nwk.txt\") as f:\n",
    "    newick = f.read()\n",
    "\n",
    "import re\n",
    "labels = re.findall(r'([A-Za-z0-9\\-\\._ ]+):', newick)\n",
    "\n",
    "labels = sorted(set(labels))\n",
    "\n",
    "with open(\"tree_labels.txt\", \"w\") as out:\n",
    "    for label in labels:\n",
    "        out.write(label + \"\\n\")\n",
    "\n",
    "print(\"Labels extraídos.\")\n"
   ]
  }
